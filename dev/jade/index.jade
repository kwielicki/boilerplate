block page_content
    .container
        .row
            .col-xs-12
                .tabs-wrapper
                    .tabs-navigation
                        li.tab-link.is-active: a(href="#tab1") Tab1
                        li.tab-link: a(href="#tab2") Tab2
                        li.tab-link: a(href="#tab3") Tab3
                    .tabs-content
                        #tab1
                            h1 Tab1
                        #tab2
                            h1 Tab2
                        #tab3
                            h1 Tab3
